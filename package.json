{
	"name": "paris-web",
	"version": "1.0.0",
	"description": "Cypress Automation Test for Paris Web",
	"main": "index.js",
	"scripts": {
		"test": "cypress open",
		"reg": "cypress run --headless",
		"reg:broker": "cypress run --spec 'cypress/e2e/Broker/*.js' --headless",
		"reg:claim": "cypress run --spec 'cypress/e2e/Claim/*.js' --headless",
		"reg:crm": "cypress run --spec 'cypress/e2e/CRM/*.js' --headless",
		"reg:offer": "cypress run --spec 'cypress/e2e/Offer/*.js' --headless",
		"reg:portal": "cypress run --spec 'cypress/e2e/PortalGeneral/*.js' --headless",
		"reg:ss": "cypress run --spec 'cypress/e2e/SelfService/*.js' --headless",
		"reg:ss:help": "cypress run --spec 'cypress/e2e/SelfService/*.js' --headless --env configFile=help",
		"smoke:claim": "cypress run --spec 'cypress/e2e/Claim/*.js' --headless --env grepTags=claim",
		"clean:report": "rimraf cypress/results cypress/results-merged",
		"json:merge": "mochawesome-merge cypress/results/*.json -o cypress/results-merged/json-merged.json",
		"xml:merge": "jrm cypress/results-merged/xml-merged.xml \"cypress/results/*.xml\"",
		"json:generate": "marge cypress/results-merged/json-merged.json -o cypress/results-merged -f finalReport --charts true --overwrite true",
		"json:2:xml": "node scripts/jsonToXML.js cypress/results-merged/json-merged.json",
		"run-with-tags": "cypress run --spec"
	},
	"keywords": [
		"paris-web",
		"cypress"
	],
	"author": "Noria QA Team",
	"license": "ISC",
	"devDependencies": {
		"@bahmutov/cy-grep": "^2.0.22",
		"@cypress/grep": "^4.1.0",
		"cypress": "12.17.4",
		"cypress-cloud": "^1.13.0",
		"cypress-file-upload": "^5.0.8",
		"cypress-multi-reporters": "^1.6.4",
		"cypress-on-fix": "^1.1.0",
		"cypress-plugin-api": "^2.11.2",
		"cypress-plugin-tab": "^1.0.5",
		"cypress-real-events": "^1.7.1",
		"cypress-wait-until": "^1.7.2",
		"find-cypress-specs": "^1.37.0",
		"junit-report-merger": "^6.0.3",
		"mocha-junit-reporter": "^2.2.1",
		"mochawesome": "^7.1.3",
		"mochawesome-merge": "^4.3.0",
		"mochawesome-report-generator": "^6.2.0",
		"rimraf": "^5.0.5",
		"xmlbuilder2": "^3.1.1"
	}
}
